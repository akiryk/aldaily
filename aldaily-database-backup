--
-- Table structure for table `wp_mfcihv_relevanssi`
--

CREATE TABLE IF NOT EXISTS `wp_mfcihv_relevanssi` (
  `id` mediumint(9) NOT NULL AUTO_INCREMENT,
  `doc` bigint(20) NOT NULL,
  `term` varchar(50) NOT NULL,
  `tf` mediumint(9) NOT NULL DEFAULT '0',
  `title` tinyint(1) NOT NULL,
  UNIQUE KEY `id` (`id`),
  KEY `terms` (`term`(20))
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 AUTO_INCREMENT=952 ;

