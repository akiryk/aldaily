--
-- Table structure for table `wp_mfcihv_relevanssi_cache`
--

CREATE TABLE IF NOT EXISTS `wp_mfcihv_relevanssi_cache` (
  `param` varchar(32) NOT NULL,
  `hits` text NOT NULL,
  `tstamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  UNIQUE KEY `param` (`param`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Dumping data for table `wp_mfcihv_relevanssi_cache`
--


-- --------------------------------------------------------

--
-- Table structure for table `wp_mfcihv_relevanssi_excerpt_cache`
--

CREATE TABLE IF NOT EXISTS `wp_mfcihv_relevanssi_excerpt_cache` (
  `query` varchar(100) NOT NULL,
  `post` mediumint(9) NOT NULL,
  `excerpt` text NOT NULL,
  UNIQUE KEY `query` (`query`,`post`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Dumping data for table `wp_mfcihv_relevanssi_excerpt_cache`
--


-- --------------------------------------------------------

--
-- Table structure for table `wp_mfcihv_relevanssi_log`
--

CREATE TABLE IF NOT EXISTS `wp_mfcihv_relevanssi_log` (
  `id` mediumint(9) NOT NULL AUTO_INCREMENT,
  `query` varchar(200) NOT NULL,
  `hits` mediumint(9) NOT NULL,
  `time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  UNIQUE KEY `id` (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

